# Supabase Configuration
# Copy this file to .env and fill in your Supabase project details

# Your Supabase project URL
EXPO_PUBLIC_SUPABASE_URL=your_supabase_project_url

# Your Supabase anon/public key
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Optional: Service role key for admin operations (keep this secret!)
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Cloudinary Configuration for Custom Symbol Storage
# Get these from https://cloudinary.com/console

# Your Cloudinary cloud name
EXPO_PUBLIC_CLOUDINARY_CLOUD_NAME=your_cloudinary_cloud_name

# Your Cloudinary API key
EXPO_PUBLIC_CLOUDINARY_API_KEY=your_cloudinary_api_key

# Your Cloudinary API secret (keep this secret!)
CLOUDINARY_API_SECRET=your_cloudinary_api_secret

# Sentry Configuration for Error Tracking
# Get your DSN from https://sentry.io/settings/projects/YOUR_PROJECT/keys/

# Your Sentry DSN (Data Source Name)
EXPO_PUBLIC_SENTRY_DSN=https://your-dsn@sentry.io/project-id

# Optional: Enable Sentry debug mode in development
# EXPO_PUBLIC_SENTRY_DEBUG=true

# Environment (development, staging, production)
NODE_ENV=development