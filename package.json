{
  "name": "ausmo",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:unit": "jest --testPathPatterns=\"src/services/__tests__\"",
    "test:integration": "jest --testPathPatterns=\"src/store/__tests__\" --testPathIgnorePatterns=\"**/e2e/**\"",
    "test:performance": "jest --testPathPatterns=\"src/__tests__/performance\"",
    "test:e2e": "detox test",
    "test:e2e:build": "detox build",
    "test:e2e:ios": "detox test --configuration ios.sim.debug",
    "test:e2e:android": "detox test --configuration android.emu.debug",
    "test:all": "node scripts/run-tests.js",
    "deploy": "node scripts/deploy.js",
    "deploy:staging": "node scripts/deploy.js staging",
    "deploy:production": "node scripts/deploy.js production",
    "deploy:config": "node scripts/deploy-config.js",
    "deploy:config:list": "node scripts/deploy-config.js list",
    "deploy:config:checklist": "node scripts/deploy-config.js checklist",
    "env:setup": "node scripts/environment-setup.js setup",
    "env:validate": "node scripts/environment-setup.js validate",
    "env:list": "node scripts/environment-setup.js list",
    "env:report": "node scripts/environment-setup.js report",
    "env:config": "node scripts/environment-setup.js config",
    "build:ios": "cd ios && xcodebuild -workspace Ausmo.xcworkspace -scheme Ausmo -configuration Release -destination 'generic/platform=iOS' archive",
    "build:android": "cd android && ./gradlew assembleRelease"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-community/netinfo": "^11.3.1",
    "@react-native-community/slider": "^5.0.1",
    "@react-native-picker/picker": "2.11.1",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/drawer": "^7.5.8",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.8",
    "@reduxjs/toolkit": "^2.9.0",
    "@supabase/supabase-js": "^2.45.4",
    "@types/crypto-js": "^4.2.2",
    "babel-preset-expo": "^54.0.1",
    "crypto-js": "^4.2.0",
    "expo": "54.0.12",
    "expo-audio": "~1.0.13",
    "expo-build-properties": "~1.0.9",
    "expo-camera": "^17.0.7",
    "expo-crypto": "~15.0.7",
    "expo-document-picker": "^14.0.7",
    "expo-file-system": "^19.0.14",
    "expo-font": "^14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image-manipulator": "^14.0.7",
    "expo-image-picker": "^17.0.8",
    "expo-linear-gradient": "^15.0.7",
    "expo-mail-composer": "^15.0.7",
    "expo-media-library": "^18.1.1",
    "expo-secure-store": "~15.0.7",
    "expo-sharing": "^14.0.7",
    "expo-speech": "^14.0.7",
    "expo-sqlite": "^16.0.8",
    "expo-status-bar": "~3.0.8",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "react-native-crypto-js": "^1.0.0",
    "react-native-draggable-flatlist": "^4.0.3",
    "react-native-elements": "^3.4.3",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-modal": "^14.0.0-rc.1",
    "react-native-paper": "^5.14.5",
    "react-native-picker-select": "^9.3.1",
    "react-native-progress": "^5.0.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.16.0",
    "react-native-slider": "^0.11.0",
    "react-native-sound": "^0.12.0",
    "react-native-super-grid": "^6.0.2",
    "react-native-svg": "15.12.1",
    "react-native-switch": "^1.5.1",
    "react-native-vector-icons": "^10.3.0",
    "react-native-worklets": "^0.5.1",
    "react-redux": "^9.2.0",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.3",
    "@types/react": "~19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "~29.7.0",
    "prettier": "^3.6.2",
    "react-refresh": "^0.17.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
